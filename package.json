{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for an restful express-apllication written in TypeScript.",
  "main": "src/index.ts",
  "scripts": {
    "install:typings": "./node_modules/.bin/typings install",
    "install:dev": "yarn install && npm run install:typings",
    "install:clean": "npm prune && ./node_modules/.bin/typings prune",
    "serve": "./node_modules/.bin/nodemon --watch 'src/**/*.ts'",
    "serve:debug": "./node_modules/.bin/nodemon --watch 'src/**/*.ts' --debug=6666",
    "lint": "./node_modules/.bin/gulp lint",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --coverage",
    "build": "./node_modules/.bin/gulp build",
    "clean": "./node_modules/.bin/gulp clean",
    "zip": "./node_modules/.bin/gulp zip",
    "docs": "npm run docs:generate && npm run docs:server",
    "docs:generate": "./node_modules/.bin/gulp docs",
    "docs:server": "./node_modules/.bin/http-server ./docs --cors",
    "db:migrate": "./node_modules/.bin/knex migrate:latest",
    "db:migrate:rollback": "./node_modules/.bin/knex migrate:rollback",
    "db:seed": "./node_modules/.bin/knex seed:run",
    "db:clean": "ts-node ./src/console/CleanDatabaseCommand.ts",
    "db:refresh": "npm run db:drop && npm run db:migrate",
    "start": "node ./src/index.js"
  },
  "repository": "git+ssh://git@github.com/w3tec/express-typescript-boilerplate.git",
  "engines": {
    "node": "^7.7.3"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate",
    "skeleton",
    "starter-kit",
    "w3tec.ch"
  ],
  "homepage": "https://github.com/w3tec/express-typescript-boilerplate#readme",
  "author": "w3tec.ch <info@w3tec.ch>",
  "contributors": [
    {
      "name": "David Weber",
      "email": "david.weber@w3tec.ch",
      "url": "https://github.com/dweber019"
    },
    {
      "name": "Gery Hirschfeld",
      "email": "gery.hirschfeld@w3tec.ch",
      "url": "https://github.com/hirsch88"
    }
  ],
  "dependencies": {
    "@types/bluebird": "^3.5.2",
    "@types/body-parser": "^1.16.3",
    "@types/bookshelf": "^0.8.38",
    "@types/cors": "^2.8.1",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.35",
    "@types/faker": "^4.1.0",
    "@types/helmet": "0.0.34",
    "@types/inversify": "^2.0.33",
    "@types/inversify-express-utils": "^2.0.0",
    "@types/knex": "0.0.47",
    "@types/lodash": "^4.14.63",
    "@types/morgan": "^1.7.32",
    "@types/reflect-metadata": "0.0.5",
    "@types/request": "0.0.42",
    "@types/serve-favicon": "^2.2.28",
    "@types/winston": "^2.3.1",
    "body-parser": "^1.17.1",
    "bookshelf": "^0.10.3",
    "bookshelf-camelcase": "^1.1.4",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "helmet": "^3.4.0",
    "inversify": "^4.1.0",
    "inversify-express-utils": "^3.5.1",
    "knex": "^0.12.7",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0",
    "pg": "^6.1.5",
    "reflect-metadata": "^0.1.10",
    "request": "^2.81.0",
    "serve-favicon": "^2.4.2",
    "ts-node": "^2.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^1.0.0",
    "gulp-filter": "^4.0.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-tslint": "^7.0.1",
    "gulp-typedoc": "^2.0.2",
    "gulp-typescript": "^3.1.4",
    "gulp-zip": "^3.2.0",
    "http-server": "^0.9.0",
    "jasmine-spec-reporter": "^4.0.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "request": "^2.79.0",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "sqlite3": "^3.1.8",
    "ts-jest": "^19.0.11",
    "tslint": "^4.3.1",
    "typedoc": "^0.5.5",
    "typescript": "^2.2.1",
    "typings": "^2.1.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "license": "MIT"
}
